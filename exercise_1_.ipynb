   "source": [
    "# Exercise 1\n",
    "Dado un número entero positivo x, mostrar su último dígito."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "The last digit is: 3\n"
     ]
    }
   ],
   "source": [
    "# Solution a\n",
    "\n",
    "num_x = int( input( \"Enter a natural number:\" ) )\n",
    "\n",
    "if num_x >= 0:\n",
    "    last_digit = num_x % 10\n",
    "    print( f\"The last digit is: {last_digit}\" )\n",
    "else:\n",
    "    print( \"Ingresa un número positivo\" )\n",
    "\n"
   ]
  }
