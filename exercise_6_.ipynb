    "# Exercise 6\n",
    "Dado un n√∫mero entero positivo, mostrar las N primeras potencias."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "2 ** 1 = 2\n",
      "2 ** 2 = 4\n",
      "2 ** 3 = 8\n",
      "2 ** 4 = 16\n",
      "2 ** 5 = 32\n"
     ]
    }
   ],
   "source": [
    "# Solution\n",
    "number_in = int( input( \"Enter a number: \" ) )\n",
    "n_pow = int( input( \"Enter a number N: \" ) )\n",
    "\n",
    "for num in range( 1, n_pow + 1 ):\n",
    "    print( f\"{number_in} ** {num} = {number_in ** num}\" )"
   ]
  }
